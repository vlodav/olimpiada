#include<iostream>
using namespace std;
int kolvo_chisel = 0;
void funk(int a)
{
	int summ = 0,rang = 1;
	while (true)
	{
		if (a < 10)
		{
			summ += a;
			break;
		}
		summ += a%10;
		a /= 10;
		rang++;
	}
	if (summ % rang == 0)
		kolvo_chisel++;
}
int main()
{
	setlocale(LC_ALL, "Russian");
	int N = 0; 
	while (true)
	{
		cout << "введите число(1...1000000): ";
		cin >> N;
		if (1 <= N && N <= 1000000)
			break;
		cout << "вы ввели недопустимое число!" << endl;
	}
	for (int i = 1; i <= N; i++)
	{
		funk(i);
	}
	cout << kolvo_chisel << endl;
	system("pause");
}
